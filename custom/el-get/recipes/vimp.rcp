(:name vimp
       :type git
       :url "git://gitorious.org/evil/evil.git"
       :features vimp
       :depends (undo-tree key-chord)
       :build/darwin (("rename 's/evil/vimp/' evil*")
		      ("rename 's/evil/vimp/' doc/*")
		      ("sed -i -e 's/evil/vimp/g' *.el")
		      ("sed -i -e 's/evil/vimp/g' Makefile")
		      ("make EMACS=/usr/local/bin/emacs all")))
